{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "index.js"
        }
    ],
    "crons": [
        {
            "path": "/api/refresh",
            "schedule": "1 0 * * *"
        }
    ]
}